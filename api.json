                                                                                            "host": "1.1.1.1"
                                                                                        }
                                                                                    }
                                                                                },
                                                                                "schema": {
                                                                                    "properties": {
                                                                                        "count": {
                                                                                            "type": "integer"
                                                                                        },
                                                                                        "host": {
                                                                                            "format": "ipv4",
                                                                                            "type": "string"
                                                                                        }
                                                                                    },
                                                                                    "required": [
                                                                                        "host",
                                                                                        "count"
                                                                                    ],
                                                                                    "type": "object"
                                                                                }
                                                                            }
                                                                        },
