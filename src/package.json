{
    "name": "mist-chrome-extension",
    "version": "2.5.0",
    "description": "Generated with @larscom/ng-chrome-extension",
    "scripts": {
        "ng": "ng",
        "start": "npm run watch",
        "build:angular": "ng build",
        "build:angular-production": "ng build --configuration production",
        "watch:angular": "ng build --source-map=true --plugin ~angular/sourcemaps-plugin.js --watch --delete-output-path false",
        "build:chrome": "webpack --config chrome/webpack.config.js",
        "build:chrome-production": "webpack --mode production --config chrome/webpack.config.prod.js",
        "watch:chrome": "webpack --config chrome/webpack.config.js -w",
        "build": "npm run clean && npm run build:angular && npm run build:chrome",
        "build:production": "npm run build:angular-production && npm run build:chrome-production && npm run pack",
        "watch": "npm run clean && npm-run-all --parallel watch:*",
        "clean": "rimraf angular/dist",
        "pack": "cd angular/dist && bestzip ../../extension-build.zip *"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "^13.3.4",
        "@angular/animations": "^13.3.5",
        "@angular/cdk": "^13.3.5",
        "@angular/cli": "^13.3.4",
        "@angular/common": "^13.3.5",
        "@angular/compiler": "^13.3.5",
        "@angular/compiler-cli": "^13.3.5",
        "@angular/core": "^13.3.4",
        "@angular/forms": "^13.3.5",
        "@angular/language-service": "^13.3.5",
        "@angular/material": "^13.3.5",
        "@angular/platform-browser": "^13.3.5",
        "@angular/platform-browser-dynamic": "^13.3.5",
        "@angular/router": "^13.3.5",
        "@types/chrome": "0.0.183",
        "@types/node": "^16.11.32",
        "awesome-typescript-loader": ">=5.2.1",
        "rxjs": "^7.5.5",
        "tslib": "^2.4.0",
        "ngx-build-plus": ">=13.0.1",
        "zone.js": "^0.11.5"
    },
    "devDependencies": {
        "ansi-html": ">=0.0.8",
        "ansi-regex": ">=5.0.1",
        "bestzip": ">=2.1.7",
        "codelyzer": ">=6.0.0",
        "dns-packet": ">=1.3.2",
        "follow-redirects": ">=1.14.8",
        "glob-parent": ">=5.1.2",
        "json-schema": ">=0.4.0",
        "nanoid": ">=3.1.31",
        "node-forge": ">=1.3.0",
        "npm-run-all": ">=4.1.5",
        "nth-check": ">=2.0.1",
        "path-parse": ">=1.0.7",
        "postcss": ">=7.0.36",
        "rimraf": ">=2.7.1",
        "tar": ">=6.1.9",
        "ts-node": ">=7.0.0",
        "url-parse": ">=1.5.9",
        "webpack": ">=5.72.0",
        "webpack-cli": ">=3.3.11",
        "ws": ">=6.2.2"
    }
}